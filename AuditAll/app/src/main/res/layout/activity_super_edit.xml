            android:layout_margin="5dp"
            android:padding="5dp"
            android:lines="4"
            android:inputType="text"
            android:imeOptions="actionNext"
            android:background="@drawable/textviewborder"
            android:textColor="@color/black"
            android:textSize="14sp" />
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_margin="5dp">

        <ImageView
            android:id="@+id/image41"
            android:layout_width="10dp"
            android:layout_alignParentLeft="true"
            android:layout_height="match_parent" />

        <TextView
            android:id="@+id/tv42"
            android:layout_width="wrap_content"
            android:layout_height="20dp"
            android:layout_margin="20dp"

            android:text="   4"
            android:layout_toRightOf="@+id/image41"
            android:layout_centerInParent="true" />
        <ImageView
            android:id="@+id/image42"
            android:layout_width="20dp"
            android:layout_toRightOf="@+id/tv42"
            android:layout_height="match_parent" />

        <EditText
            android:id="@+id/editText_superEditActivity04"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_centerInParent="true"
            android:layout_toRightOf="@+id/image42"
            android:layout_margin="5dp"
            android:padding="5dp"
            android:lines="4"
            android:inputType="text"
            android:imeOptions="actionNext"
            android:background="@drawable/textviewborder"
            android:textColor="@color/black"
            android:textSize="14sp" />
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_margin="5dp">

        <ImageView
            android:id="@+id/image51"
            android:layout_width="10dp"
            android:layout_alignParentLeft="true"
            android:layout_height="match_parent" />

        <TextView
            android:id="@+id/tv52"
